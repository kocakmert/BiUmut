<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".maps">
    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"/>
    <RelativeLayout
        android:id="@+id/back_relative"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/border_low_strong"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp">
        <Button
            android:layout_centerInParent="true"
            android:id="@+id/btn_back"
            android:background="@drawable/ic_action_namemain"
            android:layout_width="41dp"
            android:layout_height="41dp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/search"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/back_relative"
        android:layout_width="match_parent"
        android:layout_height="41dp"
        android:background="@drawable/maps_back">
        <fragment
            android:id="@+id/auto_complete"
            android:name="com.google.android.libraries.places.widget.AutocompleteSupportFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <Button
        android:layout_below="@+id/search"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:id="@+id/btn"
        android:background="@drawable/ic_action_nameloc"/>
    <Button
        android:layout_below="@+id/search"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:id="@+id/btn_user_Ä±stek"
        android:background="@drawable/ic_action_nameloc"
        android:layout_alignParentRight="true" />
</RelativeLayout>